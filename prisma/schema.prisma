

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model contactus {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  phone     Int
  email     String
  message   String
  createdAt   DateTime @default(now())
}

model Register {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  username    String?  @unique
  email       String?  @unique
  password    String?
  comments    String?
  AskQuestion String?
  createdAt   DateTime @default(now())
  updateAt    DateTime @updatedAt
}

model broadcast {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  message   String
  createdAt DateTime @default(now())
}
